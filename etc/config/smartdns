
config smartdns
	option server_name 'smartdns'
	option port '6053'
	option server_group 'mainland'
	option redirect 'none'
	option ipv6_server '1'
	option rr_ttl_min '300'
	option prefetch_domain '1'
	option serve_expired '1'
	option coredump '0'
	option enabled '1'
	option force_aaaa_soa '1'
	option cache_size '1000'
	option tcp_server '0'
	option seconddns_port '7053'
	option seconddns_no_speed_check '1'
	option seconddns_no_dualstack_selection '1'
	option seconddns_no_rule_addr '0'
	option seconddns_no_rule_nameserver '0'
	option seconddns_no_rule_ipset '0'
	option seconddns_no_rule_soa '0'
	option seconddns_no_cache '0'
	option seconddns_enabled '1'
	option seconddns_server_group 'overseas'
	option seconddns_tcp_server '0'
	list old_redirect 'none'
	list old_port '6053'
	list old_enabled '1'

config server
	option name 'Google HTTPS'
	option ip 'https://dns.google/dns-query'
	option type 'https'
	option server_group 'overseas'
	option addition_arg '-exclude-default-group'
	option tls_host_verify 'dns.google'
	option no_check_certificate '0'
	option blacklist_ip '0'
	option enabled '1'

config server
	option name 'Google TLS#1'
	option ip '8.8.8.8'
	option type 'tls'
	option server_group 'overseas'
	option addition_arg '-exclude-default-group'
	option tls_host_verify 'dns.google'
	option no_check_certificate '0'
	option blacklist_ip '0'
	option enabled '1'

