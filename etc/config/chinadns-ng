
config chinadns-ng
	option enable '1'
	option bind_addr '0.0.0.0'
	option china_dns '127.0.0.1#8053'
	option trust_dns '127.0.0.1#7053'
	option ipset_name_4 'chnroute'
	option ipset_name_6 'chnroute6'
	option gfwlist_file '/etc/chinadns-ng/gfwlist.txt'
	option chnlist_file '/etc/chinadns-ng/chinalist.txt'
	option timeout_sec '2'
	option repeat_times '2'
	option reuse_port '1'
	option noip_as_chnip '0'
	option bind_port '5335'
	option fair_mode '0'
	option chnlist_first '0'

